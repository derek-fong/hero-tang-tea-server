{
  "name": "hero-tang-tea-server",
  "version": "1.5.0",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "apollo:service:push": "apollo service:push --endpoint=$APOLLO_ENGINE_ENDPOINT_URL --key=$APOLLO_ENGINE_API_KEY --tag=$APOLLO_ENGINE_SCHEMA_TAG",
    "apollo:service:push:delay": "npm-run-all --serial delay apollo:service:push",
    "build": "webpack --config webpack.$NODE_ENV.js",
    "delay": "delay ${NPM_SCRIPTS_DELAY_SECONDS:-0}",
    "lint": "eslint",
    "start": "npm-run-all --parallel start:server apollo:service:push:delay",
    "start:env": "npm-run-all --parallel start:server:env apollo:service:push:delay",
    "start:server": "node ./dist/server",
    "start:server:env": "node --require dotenv/config ./dist/server",
    "test": "jest"
  },
  "repository": "github:alppix/hero-tang-tea-server",
  "dependencies": {
    "@graphql-modules/core": "^0.7.9",
    "@okgrow/graphql-scalars": "^0.4.5",
    "@types/graphql-iso-date": "^3.3.1",
    "apollo": "^2.17.4",
    "apollo-server-express": "^2.7.2",
    "axios": "^0.19.0",
    "cors": "^2.8.5",
    "delay-cli": "^1.1.0",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "graphql": "^14.4.2",
    "graphql-iso-date": "^3.6.1",
    "graphql-voyager": "^1.0.0-rc.27",
    "jwks-rsa": "^1.6.0",
    "mongoose": "^5.5.4",
    "npm-run-all": "^4.1.5",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@semantic-release/changelog": "^3.0.4",
    "@semantic-release/git": "^7.0.16",
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.5",
    "@types/express": "^4.17.1",
    "@types/express-jwt": "0.0.42",
    "@types/graphql-iso-date": "^3.3.1",
    "@types/jest": "^24.0.18",
    "@types/mongoose": "^5.3.27",
    "@types/node": "^11.13.8",
    "@types/webpack-env": "^1.14.0",
    "@typescript-eslint/eslint-plugin": "^1.12.0",
    "@typescript-eslint/parser": "^1.12.0",
    "apollo-server-testing": "^2.7.0",
    "clean-webpack-plugin": "^3.0.0",
    "dotenv": "^8.0.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^3.0.1",
    "jest": "^24.8.0",
    "jest-junit": "^6.4.0",
    "jest-transform-graphql": "^2.1.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "semantic-release": "^15.13.18",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.3",
    "webpack": "^4.36.1",
    "webpack-cli": "^3.3.6",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2"
  }
